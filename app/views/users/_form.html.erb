<%= form_for @user do |f| %>
<%= f.label :email %>
<%= f.text_field :email %>
<%= f.label :first_name %>
<%= f.text_field :first_name %>
<%= f.label :last_name %>
<%= f.text_field :last_name %>
<%= f.label :address %>
<%= f.text_field :address %>
<%= f.label :enabled %>
<%= f.check_box :enabled %>
<%= f.submit %>
<% end %>
